{
    "name": "lumturo-net/article-config-bundle",
    "type": "contao-bundle",
    "description": "Article config bundle for Contao4",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Rolf Staege"
        },
        {
            "name": "Christian Wederka"
        }
    ],
    "require": {
        "php": "^7.2",
        "contao/core-bundle": "4.9.*",
        "symfony/framework-bundle": "^4.2"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "lumturo\\ArticleConfigBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "lumturo\\ArticleConfigBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lumturo\\ArticleConfigBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/contao/skeleton-bundle/issues",
        "source": "https://github.com/contao/skeleton-bundle"
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ tests/ --fix --ansi"
        ]
    }
}
